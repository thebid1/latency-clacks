# ==========================================
# CONTACT FORM CONFIGURATION
# ==========================================
# Get your Formspree endpoint from https://formspree.io
# This is safe to expose publicly - it's meant for browser submissions
NEXT_PUBLIC_FORMSPREE_ENDPOINT=https://formspree.io/f/YOUR_FORM_ID

# ==========================================
# CONTACT EMAIL
# ==========================================
# Public contact email displayed on the site
NEXT_PUBLIC_CONTACT_EMAIL=your-email@example.com

# ==========================================
# SITE CONFIGURATION
# ==========================================
# Your production domain (used for SEO/canonical URLs)
NEXT_PUBLIC_SITE_URL=https://your-domain.com

